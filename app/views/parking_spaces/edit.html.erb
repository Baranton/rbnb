<h2>Edit parking space located at @parking_space.address</h2>

<%= simple_form_for @parking_space do |f| %>
  <%= f.input :address %>
  <%= f.input :postcode %>
  <%= f.input :city %>
  <%= f.input :country, as: :text %>
  <%= f.input :latitude %>
  <%= f.input :longitude %>
  <%= f.input :category, as: :text %>
  <%= f.input :availability %>
  <%= f.input :size %>
  <%= f.input :price_per_hour %>

  <%= f.submit "Edit parking space", class: "btn btn-primary" %>
<% end %>




