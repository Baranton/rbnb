            <div class="content-card-content"> -->
              <%= yield %>
           <!-- </div>
          </div> -->
        </div>
     <!--  </div>
    </div> -->
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?libraries=places&key=#{ENV['GOOGLE_API_CLIENT_KEY']}" %>
    <%= javascript_include_tag 'application' %>
    <%= javascript_pack_tag 'application' %>
    <%= render 'shared/footer' %>
  </body>
</html>
